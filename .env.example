# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google Cloud Platform Configuration
GCP_PROJECT_ID=your_project_id
GCP_LOCATION=us-central1

# Legacy API Keys (will be migrated to Firebase)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# ElevenLabs API Configuration (for voice synthesis)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Supabase Configuration (for database)
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# WebRTC Configuration (for peer support)
VITE_WEBRTC_SERVER_URL=wss://your-webrtc-server.com

# Instructions:
# 1. Copy this file to .env.local
# 2. Create Firebase project: https://console.firebase.google.com
# 3. Get Firebase config from Project Settings > General > Your apps
# 4. Replace all placeholder values with your actual credentials
# 5. Never commit .env.local to version control

# Optional: Development settings
VITE_DEV_MODE=true
VITE_MOCK_DATA=false
VITE_ENABLE_ANALYTICS=true
